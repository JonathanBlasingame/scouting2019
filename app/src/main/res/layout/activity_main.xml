<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteY="25dp">

    <CheckBox
        android:id="@+id/RLH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/Right"
        app:layout_constraintEnd_toEndOf="@+id/CLH"
        app:layout_constraintStart_toStartOf="@+id/CLH" />

    <CheckBox
        android:id="@+id/CLH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/Center"
        app:layout_constraintEnd_toEndOf="@+id/LH"
        app:layout_constraintStart_toStartOf="@+id/LH" />

    <CheckBox
        android:id="@+id/blueAlliance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="Blue Alliance"
        android:textColor="#0037b1"
        app:layout_constraintBaseline_toBaselineOf="@+id/RedAlliance"
        app:layout_constraintEnd_toEndOf="parent" />

    <CheckBox
        android:id="@+id/RedAlliance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Red Alliance"
        android:textColor="@color/colorAccent"
        app:layout_constraintEnd_toStartOf="@+id/blueAlliance"
        app:layout_constraintStart_toEndOf="@+id/teamNumber"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/teamNumber"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@android:drawable/edit_text"
        android:ems="10"
        android:hint="Team #"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="@+id/matchNumber"
        app:layout_constraintStart_toEndOf="@+id/matchNumber"
        app:layout_constraintTop_toTopOf="@+id/matchNumber" />

    <EditText
        android:id="@+id/ScouterName"
        android:layout_width="68dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@android:drawable/edit_text"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/svision"
        style="@android:style/Widget.Button.Toggle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@android:drawable/button_onoff_indicator_off"
        android:text="vision"
        android:textAppearance="@style/TextAppearance.AppCompat.Button"
        app:layout_constraintBottom_toTopOf="@+id/sandHatchA"
        app:layout_constraintStart_toEndOf="@+id/sandBallA"
        app:layout_constraintTop_toTopOf="@+id/sandBallA" />

    <Button
        android:id="@+id/m_rocket"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="middle"
        app:layout_constraintEnd_toEndOf="@+id/t_rocket"
        app:layout_constraintTop_toBottomOf="@+id/t_rocket" />

    <Button
        android:id="@+id/b_rocket"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="bottom"
        app:layout_constraintEnd_toEndOf="@+id/m_rocket"
        app:layout_constraintTop_toBottomOf="@+id/m_rocket" />

    <Button
        android:id="@+id/t_rocket"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="308dp"
        android:text="Top"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/matchNumber"
        style="@android:style/Widget.AutoCompleteTextView"
        android:layout_width="84dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@android:drawable/edit_text"
        android:ems="10"
        android:hint="Match "
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="@+id/ScouterName"
        app:layout_constraintStart_toEndOf="@+id/ScouterName" />

    <Button
        android:id="@+id/sauto"
        style="@android:style/Widget.Button.Toggle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:drawable/button_onoff_indicator_off"
        android:text="Auto"
        app:layout_constraintBottom_toBottomOf="@+id/sandHatchA"
        app:layout_constraintEnd_toEndOf="@+id/svision"
        app:layout_constraintStart_toStartOf="@+id/svision"
        app:layout_constraintTop_toTopOf="@+id/sandHatchA" />

    <CheckBox
        android:id="@+id/ci"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Communication issues?"
        app:layout_constraintBottom_toBottomOf="@+id/sauto"
        app:layout_constraintEnd_toEndOf="@+id/dbbp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/dbbp"
        app:layout_constraintTop_toBottomOf="@+id/dbbp" />

    <CheckBox
        android:id="@+id/dbbp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Dropped Battery/Bumpers/Parts?"
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toTopOf="@+id/sauto"
        app:layout_constraintStart_toEndOf="@+id/svision"
        app:layout_constraintTop_toTopOf="@+id/svision" />

    <CheckBox
        android:id="@+id/bd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Broke Down?"
        app:layout_constraintBottom_toBottomOf="@+id/sauto"
        app:layout_constraintEnd_toEndOf="@+id/ci"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ci"
        app:layout_constraintTop_toBottomOf="@+id/ci"
        app:layout_constraintVertical_bias="0.0" />

    <CheckBox
        android:id="@+id/fo"
        android:layout_width="wrap_content"
        android:layout_height="59dp"
        android:layout_marginStart="24dp"
        android:text="Fell Over?"
        app:layout_constraintBottom_toBottomOf="@+id/ci"
        app:layout_constraintStart_toEndOf="@+id/ci"
        app:layout_constraintTop_toTopOf="@+id/ci" />

    <TextView
        android:id="@+id/Left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Left"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/b_rocket"
        app:layout_constraintTop_toBottomOf="@+id/b_rocket" />

    <TextView
        android:id="@+id/Center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Center"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Left" />

    <TextView
        android:id="@+id/Right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Right"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/Center"
        app:layout_constraintTop_toBottomOf="@+id/Center" />

    <TextView
        android:id="@+id/Front"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Front"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/Right"
        app:layout_constraintStart_toStartOf="@+id/Right"
        app:layout_constraintTop_toBottomOf="@+id/Right" />

    <EditText
        android:id="@+id/finalScore"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="700dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:background="@android:drawable/edit_text"
        android:ems="10"
        android:hint="Final Score "
        android:inputType="number"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/notes"
        android:layout_width="177dp"
        android:layout_height="wrap_content"
        android:background="@android:drawable/edit_text"
        android:ems="10"
        android:hint="Notes"
        android:inputType="textPersonName"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/finalScore"
        app:layout_constraintEnd_toStartOf="@+id/finalScore"
        app:layout_constraintTop_toTopOf="@+id/finalScore" />

    <Button
        android:id="@+id/StandStorm"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="SandStorm"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/teamNumber"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/matchNumber" />

    <Spinner
        android:id="@+id/wlt"
        android:layout_width="102dp"
        android:layout_height="41dp"
        android:layout_marginStart="8dp"
        android:background="@android:drawable/spinner_dropdown_background"
        android:entries="@array/wltValues"
        android:popupBackground="@android:drawable/spinner_dropdown_background"
        app:layout_constraintBottom_toBottomOf="@+id/notes"
        app:layout_constraintEnd_toStartOf="@+id/notes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/notes" />

    <CheckBox
        android:id="@+id/RTopleft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:button="@drawable/custom_chk"
        app:layout_constraintBottom_toBottomOf="@+id/t_rocket"
        app:layout_constraintStart_toEndOf="@+id/t_rocket"
        app:layout_constraintTop_toTopOf="@+id/t_rocket" />

    <CheckBox
        android:id="@+id/RTopright"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/RTopleft"
        app:layout_constraintStart_toEndOf="@+id/BtnTN" />

    <CheckBox
        android:id="@+id/RTopleft1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:button="@drawable/custom_chk"
        app:layout_constraintBottom_toBottomOf="@+id/RTopright"
        app:layout_constraintStart_toEndOf="@+id/RTopright"
        app:layout_constraintTop_toTopOf="@+id/RTopright" />

    <CheckBox
        android:id="@+id/RTopright1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/custom_chk"
        app:layout_constraintBottom_toBottomOf="@+id/RTopleft1"
        app:layout_constraintStart_toEndOf="@+id/BtnTnr"
        app:layout_constraintTop_toTopOf="@+id/RTopleft1" />

    <CheckBox
        android:id="@+id/Rmiddleleft1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/RmiddleRight"
        app:layout_constraintStart_toEndOf="@+id/RmiddleRight" />

    <CheckBox
        android:id="@+id/RmiddleLeft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/m_rocket"
        app:layout_constraintEnd_toEndOf="@+id/RTopleft"
        app:layout_constraintStart_toStartOf="@+id/RTopleft" />

    <CheckBox
        android:id="@+id/RmiddleRight1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/BtnMNr"
        app:layout_constraintEnd_toEndOf="@+id/RTopright1"
        app:layout_constraintStart_toEndOf="@+id/BtnTnr" />

    <CheckBox
        android:id="@+id/RmiddleRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/BtnMN"
        app:layout_constraintEnd_toEndOf="@+id/RTopright"
        app:layout_constraintStart_toEndOf="@+id/BtnTN" />

    <CheckBox
        android:id="@+id/RBottoml"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/b_rocket"
        app:layout_constraintEnd_toEndOf="@+id/RmiddleLeft"
        app:layout_constraintStart_toStartOf="@+id/RmiddleLeft" />

    <CheckBox
        android:id="@+id/RBottomRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/BtnBN"
        app:layout_constraintEnd_toEndOf="@+id/RmiddleRight"
        app:layout_constraintStart_toStartOf="@+id/RmiddleRight" />

    <CheckBox
        android:id="@+id/RBottomleft1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/RBottomRight"
        app:layout_constraintStart_toEndOf="@+id/RBottomRight" />

    <CheckBox
        android:id="@+id/RBottomRight1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/custom_chk"
        app:layout_constraintBaseline_toBaselineOf="@+id/BtnBNr"
        app:layout_constraintEnd_toEndOf="@+id/RmiddleRight1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/RmiddleRight1" />

    <Button
        android:id="@+id/BtnTP"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="43dp"
        android:text="+"
        android:textAlignment="viewEnd"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/RTopleft"
        app:layout_constraintStart_toEndOf="@+id/RTopleft"
        app:layout_constraintTop_toTopOf="@+id/RTopleft" />

    <Button
        android:id="@+id/btnTPr"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="43dp"
        android:text="+"
        android:textAlignment="viewEnd"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/RTopleft1"
        app:layout_constraintStart_toEndOf="@+id/RTopleft1"
        app:layout_constraintTop_toTopOf="@+id/RTopleft1" />

    <TextView
        android:id="@+id/tn"
        android:layout_width="32dp"
        android:layout_height="41dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/BtnTP"
        app:layout_constraintStart_toEndOf="@+id/BtnTP" />

    <Button
        android:id="@+id/BtnTN"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="33dp"
        android:layout_height="43dp"
        android:text="-"
        android:textAlignment="viewStart"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tn"
        app:layout_constraintStart_toEndOf="@+id/tn"
        tools:text="-" />

    <Button
        android:id="@+id/BtnTnr"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="33dp"
        android:layout_height="43dp"
        android:text="-"
        android:textAlignment="viewStart"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tnr"
        app:layout_constraintStart_toEndOf="@+id/tnr"
        tools:text="-" />

    <TextView
        android:id="@+id/tnr"
        android:layout_width="32dp"
        android:layout_height="41dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/btnTPr"
        app:layout_constraintStart_toEndOf="@+id/btnTPr" />

    <ImageButton
        android:id="@+id/s1"
        style="@android:style/Widget.ImageButton"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/img_buttons"
        app:layout_constraintEnd_toEndOf="@+id/finalScore"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/finalScore"
        app:layout_constraintTop_toBottomOf="@+id/finalScore"
        tools:src="@android:drawable/ic_menu_save" />

    <Button
        android:id="@+id/BtnMP"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="+"
        android:textAlignment="viewStart"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/RmiddleLeft"
        app:layout_constraintEnd_toStartOf="@+id/tn"
        app:layout_constraintStart_toEndOf="@+id/RmiddleLeft" />

    <TextView
        android:id="@+id/Mn"
        android:layout_width="32dp"
        android:layout_height="41dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/BtnMP"
        app:layout_constraintStart_toEndOf="@+id/BtnMP" />

    <Button
        android:id="@+id/BtnMN"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="41dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="-"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/Mn"
        app:layout_constraintEnd_toStartOf="@+id/RmiddleRight"
        app:layout_constraintStart_toEndOf="@+id/Mn" />

    <Button
        android:id="@+id/BtnBP"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="43dp"
        android:text="+"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/RBottoml"
        app:layout_constraintEnd_toEndOf="@+id/BtnMP"
        app:layout_constraintStart_toStartOf="@+id/BtnMP" />

    <TextView
        android:id="@+id/Bn"
        android:layout_width="32dp"
        android:layout_height="41dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/BtnBP"
        app:layout_constraintEnd_toEndOf="@+id/Mn"
        app:layout_constraintStart_toStartOf="@+id/Mn" />

    <Button
        android:id="@+id/BtnBN"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="41dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="-"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/Bn"
        app:layout_constraintEnd_toStartOf="@+id/RmiddleRight"
        app:layout_constraintStart_toEndOf="@+id/Mn" />

    <TextView
        android:id="@+id/nmr"
        android:layout_width="32dp"
        android:layout_height="41dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/BtnMPr"
        app:layout_constraintEnd_toEndOf="@+id/tnr"
        app:layout_constraintStart_toStartOf="@+id/tnr" />

    <Button
        android:id="@+id/BtnMPr"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="41dp"
        android:text="+"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/Rmiddleleft1"
        app:layout_constraintStart_toEndOf="@+id/Rmiddleleft1" />

    <Button
        android:id="@+id/BtnMNr"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="41dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="-"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/nmr"
        app:layout_constraintEnd_toStartOf="@+id/RmiddleRight1"
        app:layout_constraintStart_toEndOf="@+id/nmr" />

    <Button
        android:id="@+id/BtnBPr"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="41dp"
        android:text="+"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/RBottomleft1"
        app:layout_constraintStart_toEndOf="@+id/RBottomleft1" />

    <TextView
        android:id="@+id/BNr"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/BtnBPr"
        app:layout_constraintEnd_toEndOf="@+id/nmr"
        app:layout_constraintStart_toEndOf="@+id/BtnBPr" />

    <Button
        android:id="@+id/BtnBNr"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="36dp"
        android:layout_height="41dp"
        android:text="-"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/BNr"
        app:layout_constraintEnd_toStartOf="@+id/RBottomRight1"
        app:layout_constraintStart_toEndOf="@+id/nmr" />

    <CheckBox
        android:id="@+id/LH"
        style="@style/Widget.AppCompat.CompoundButton.CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/custom_chk"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBaseline_toBaselineOf="@+id/Left"
        app:layout_constraintEnd_toEndOf="@+id/RBottoml"
        app:layout_constraintStart_toStartOf="@+id/RBottoml" />

